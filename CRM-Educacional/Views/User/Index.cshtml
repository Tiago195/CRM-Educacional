@model List<UserModel>

<section>
  <h1>Lista de candidatos</h1>
  <table class="table table-striped table-bordered table-hover">
    <thead class="thead-dark">
      <tr>
        <th>#</th>
        <th>Nome</th>
        <th>CPF</th>
        <th>Ações </th>
      </tr>
    </thead>
    <tbody>
      @for (int i = 0; i < Model.Count; i++)
      {
        var index = i + 1;
        <tr>
          <td>@index</td>
          <td>@Model[i].Name</td>
          <td>@Model[i].CPF</td>
          <td>
            <a asp-controller="User" asp-route-id=@Model[i].Id asp-action="Info"
            class="btn btn-outline-primary">Detalhes</a>
          </td>
        </tr>
      }
    </tbody>
  </table>
</section>